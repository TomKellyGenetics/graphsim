@article{Collingridge2013,
author = {Dave S. Collingridge},
title = {A Primer on Quantitized Data Analysis and Permutation Testing},
journal = {Journal of Mixed Methods Research},
volume = {7},
number = {1},
pages = {81-97},
year = {2013},
doi = {10.1177/1558689812454457},

URL = { 
        http://dx.doi.org/10.1177/1558689812454457
    
},
eprint = { 
        http://dx.doi.org/10.1177/1558689812454457
    
}
,
    abstract = { Quantitization refers to transforming qualitative data into numerical format. Approaches to quantitization include dichotomizing qualitative themes and counting qualitative codes. Statistical analysis of themes and code counts has the potential to produce valuable information for mixed methods researchers. Examples of quantitized statistical analysis are presented along with ways in which the results can provide useful insights. With regard to analyzing code counts, due to limitations associated with traditional parametric and nonparametric tests of significance, permutation testing is offered as a rigorous alternative. A detailed example of permutation testing using readily available resampling software is provided. }
}


